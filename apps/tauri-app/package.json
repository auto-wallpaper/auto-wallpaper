{
  "name": "@acme/tauri-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf .next .turbo node_modules",
    "next:build": "pnpm with-env next build",
    "next:dev": "pnpm with-env next dev --port 4000",
    "next:start": "pnpm with-env next start",
    "dev": "pnpm tauri dev",
    "build": "pnpm tauri build",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./src-tauri/.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@acme/ui": "workspace:^0.1.0",
    "@aptabase/tauri": "github:aptabase/tauri-plugin-aptabase#v2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@mantine/hooks": "^7.5.1",
    "@next/bundle-analyzer": "^14.1.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^5.18.0",
    "@tanstack/react-query-devtools": "^5.17.7",
    "@tanstack/react-query-next-experimental": "5.18.0",
    "@tauri-apps/api": "2.0.0-rc.0",
    "@tauri-apps/plugin-autostart": "2.0.0-rc.0",
    "@tauri-apps/plugin-dialog": "2.0.0-rc.0",
    "@tauri-apps/plugin-fs": "2.0.0-rc.0",
    "@tauri-apps/plugin-http": "2.0.0-rc.0",
    "@tauri-apps/plugin-log": "2.0.0-rc.0",
    "@tauri-apps/plugin-process": "2.0.0-rc.0",
    "@tauri-apps/plugin-store": "2.0.0-rc.0",
    "@tauri-apps/plugin-updater": "2.0.0-rc.0",
    "compare-versions": "^6.1.0",
    "compressorjs": "^1.2.1",
    "geist": "^1.2.1",
    "next": "^14.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@tauri-apps/cli": "2.0.0-rc.3",
    "@types/node": "^20.11.13",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "type-fest": "^4.10.2",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base",
      "@acme/eslint-config/nextjs",
      "@acme/eslint-config/react"
    ]
  },
  "prettier": "@acme/prettier-config"
}
